                    android:layout_height="1dp"
                    android:layout_marginTop="20dp"
                    android:background="@color/Viewc" />
                <LinearLayout
                    android:id="@+id/mi_gsmc"
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/bg_item" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/margin_left2"
                        android:src="@drawable/icon_gsmc" />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="7dp"
                        android:gravity="center_vertical"
                        android:singleLine="true"
                        android:text="公司名称"
                        android:textColor="@color/text292929"
                        android:textSize="17sp" />

                    <EditText
                        android:id="@+id/et_company_name"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:singleLine="true"
                        android:text=""
                        android:textColor="@color/text6c6c6c6"
                        android:background="@color/white"
                        android:layout_marginRight="20dp"
                        android:textSize="13sp" />
                </LinearLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="@color/Viewc" />
                <LinearLayout
                    android:id="@+id/mi_djh"
