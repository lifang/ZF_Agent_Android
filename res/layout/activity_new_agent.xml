                <EditText
                    android:id="@+id/et_password_confirm"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="15dp"
                    android:background="@color/white"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:password="true"
                    android:hint="确认密码"
                    android:textColor="@color/text292929"
                    android:textSize="17sp" />
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/Viewc" />
            <LinearLayout  android:layout_height="42dp"
                android:layout_width="fill_parent"
                android:orientation="horizontal"
                android:background="@color/F3F2F2"
                android:gravity="center_vertical">

                <CheckBox
                    android:id="@+id/cb_is_profit"
                    android:button="@drawable/checkbox_selector"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="20dp"
                    android:checked="false" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="7dp"
                    android:text="开通分润"
                    android:textColor="@color/text6c6c6c6"
                    />


            </LinearLayout>

            <Button
                android:id="@+id/btn_submit"
                android:layout_height="42dp"
                android:layout_width="184dp"
                android:layout_marginTop="50dp"
                android:layout_marginBottom="50dp"
                android:text="创建"
                android:background="@drawable/normal_button_selector"
                android:layout_gravity="center_horizontal"
                android:textColor="@color/ffffff"
                android:textSize="20sp"/>


